Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"envelope", "[", 
    RowBox[{"t_", ",", "duration_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Log", "[", 
       RowBox[{"t", "+", "0.001"}], "]"}], "-", 
      RowBox[{"Log", "[", "0.001", "]"}]}], ")"}], " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "4"}], "/", "duration"}], " ", "t"}]]}]}], ";"}]], "Input",
