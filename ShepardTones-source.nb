Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"harmonicAmplitude", "[", "f_", "]"}], ":=", 
   RowBox[{"N", "[", 
    RowBox[{"1", "-", 
     RowBox[{"Cos", "[", 
      FractionBox[
       RowBox[{"2", " ", "\[Pi]", " ", 
        RowBox[{"Log", "[", 
         FractionBox["f", "263"], "]"}]}], 
       RowBox[{"Log", "[", 
        SuperscriptBox["2", "5"], "]"}]], "]"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{3.35696210375764*^9, 3.36465134528368*^9},
 ImageRegion->{{0, 1}, {0, 1}},
