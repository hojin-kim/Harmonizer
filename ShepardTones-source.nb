Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"tone", "[", 
    RowBox[{"t_", ",", "m_", ",", "duration_"}], "]"}], ":=", 
   RowBox[{"N", "[", 
    RowBox[{
     RowBox[{"envelope", "[", 
      RowBox[{"t", ",", "duration"}], "]"}], "*", " ", 
     RowBox[{"Total", "[", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", 
          RowBox[{
           SuperscriptBox["2.", "m"], " ", "263", " ", 
           SuperscriptBox["2.", "n"], " ", "t"}], "]"}], " ", 
         RowBox[{"harmonicAmplitude", "[", 
          RowBox[{
           SuperscriptBox["2.", "m"], " ", "263", " ", 
           SuperscriptBox["2.", "n"]}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "0", ",", "4"}], "}"}]}], "]"}], "]"}]}], "]"}]}], 
